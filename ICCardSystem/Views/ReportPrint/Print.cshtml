@{
    Layout = null;
}

<html>
<head>
    <title></title>
    <script src="~/Scripts/jquery.PrintArea.js"></script>
</head>

<body>
    <div id="myPrintArea" style="margin-top:50px;margin-left:50px">
        <div>
            <span style="display:inline-block;width:50px;">(补)</span>
            <span style="display:inline-block;width:100px;">@ViewBag.gqrq</span>
            <span style="display:inline-block;width:100px;">@ViewBag.fpbh</span>
        </div>
        <div>
            <span style="display:inline-block;width:150px;">编号：@ViewBag.ywbh</span>
            <span style="display:inline-block;width:100px;">姓名：@ViewBag.hm</span>
            <span style="display:inline-block;width:100px;">操作员：@ViewBag.czy</span>
        </div>
        <div>
            <span>@ViewBag.zz</span>
        </div>
        <br />
        <br />
        <br />
        <br />
        <div>
            <span style="display:inline-block;width:150px;"></span>
            <span style="display:inline-block;width:150px;">@ViewBag.gql</span>
            <span style="display:inline-block;width:150px;">@ViewBag.qjMoney</span>
        </div>
        <div>
            <span style="display:inline-block;width:100px;"></span>
            <span style="display:inline-block;width:150px;">缴费金额 @ViewBag.moneyTotal</span>
            <span style="display:inline-block;width:150px;">找零 @ViewBag.returnMoney</span>
        </div>
        <br>
        <br>
        <div>
            <span style="display:inline-block;width:150px;"></span>
            <span style="display:inline-block;width:100px;">单价：<span>@ViewBag.qj</span></span>
        </div>      
    </div>  
    <div>
        <button id="btnPrint" class="btn btn-default" style="margin-left:110px" type="button">打 印</button>
    </div>
    <script type="text/javascript">
        $(function () {
            //打印
            $("#btnPrint").bind("click", function (event) {
                $("#myPrintArea").printArea();
            });
        });
    </script>

      

</body>
</html>
